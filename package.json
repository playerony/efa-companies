{
  "name": "efa-companies",
  "version": "0.1.0",
  "private": true,
  "typings": "./src/types.d.ts",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/node": "^12.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "4.2.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "eject": "react-scripts eject",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "lint": "eslint --ext .ts \"src/**/*\"",
    "format": "prettier --write \"src/**/*\"",
    "typecheck": "tsc --noEmit -p . --pretty",
    "build-storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 6006 -s public",
    "lint:fix": "eslint --fix --ext .ts \"src/**/*\"",
    "reinstall-deps": "bash ./scripts/reinstall-deps.sh",
    "test:e2e": "npx cypress run --spec './cypress/integration/**/*.spec.*'",
    "test": "NODE_ENV=test jest --config ./jest.config.js --env=jest-environment-jsdom-sixteen",
    "test:watch": "NODE_ENV=test jest --config ./jest.config.js --watch --env=jest-environment-jsdom-sixteen",
    "test:ci": "NODE_ENV=test jest --config ./jest.config.js --ci --coverage --forceExit --env=jest-environment-jsdom-sixteen"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@storybook/addon-actions": "^6.2.7",
    "@storybook/addon-links": "^6.2.7",
    "@storybook/addons": "^6.2.8",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.7",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/react-hooks": "^5.1.1",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@types/testing-library__cypress": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "antd": "^4.16.1",
    "axios-mock-adapter": "^1.19.0",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "^1.13.3",
    "customize-cra": "^1.0.0",
    "cypress": "^7.1.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "less": "^4.1.1",
    "less-loader": "^9.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-app-rewire-alias": "^1.0.3",
    "react-app-rewired": "^2.1.8",
    "styled-components": "^5.2.3",
    "ts-jest": "^26.5.4"
  }
}
